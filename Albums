<body onload="tableSetter()">
<header>
<script type="text/javascript">
/*JS CODE*/
function lp(title, artist, releaseDate, albumSrc){
	this.title = title;
	this.artist = artist;
	this.releaseDate = releaseDate;
	this.albumSrc = albumSrc;
};

var AfterTheGoldRush = new lp("After the Gold Rush", "Neil Young", 1970, "BigAfterTheGoldRush.jpg"); 
var AgentsOfFortune = new lp("Agents of Fortune", "Blue Oyster Cult", 1976, "BigAgentsOfFortune.jpg"); 
var Aja = new lp("Aja", "Steely Dan", 1977, "BigAja.jpg"); 
var AladdinSane = new lp("Aladdin Sane", "David Bowie", 1973, "BigAladdinSane.jpg"); 
