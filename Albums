var Hemispheres = new lp("Hemispheres", "Rush", 1978, "BigHemispheres.jpg"); 
var InTheCourtOfTheCrimsonKing = new lp("In the Court of the Crimson King", "King Crimson", 1969, "BigInTheCourtOfTheCrimsonKing.jpg"); 
var LAWoman = new lp("L.A. Woman", "The Doors", 1971, "BigLAWoman.jpg"); 
var LedZeppelinIV = new lp("Led Zeppelin IV", "Led Zeppelin", 1971, "BigLedZeppelinIV.jpg"); 
