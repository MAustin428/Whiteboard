var TheRiseAndFallOfZiggyStardustAndTheSpidersFromMars = new lp("The Rise and Fall of Ziggy Stardust and the Spiders from Mars", "David Bowie", 1972, "BigTheRiseAndFallOfZiggyStardustAndTheSpidersFromMars.jpg"); 
var TheWall = new lp("The Wall", "Pink Floyd", 1979, "BigTheWall.jpg"); 
var TheYardbirdsGreatestHits = new lp("The Yardbirds Greatest Hits", "The Yardbirds", 1977, "BigTheYardbirdsGreatestHits.jpg"); 
var TheYesAlbum = new lp("The Yes Album", "Yes", 1971, "BigTheYesAlbum.jpg"); 
var Thriller = new lp("Thriller", "Michael Jackson", 1982, "BigThriller.jpg"); 
var WhosNext = new lp("Who's Next", "The Who", 1971, "BigWhosNext.jpg"); 
var YouDontMessAroundwithJim = new lp("You Don't Mess Around with Jim", "Jim Croce", 1972, "BigYouDontMessAroundwithJim.jpg");

var lpArray = [AfterTheGoldRush, AgentsOfFortune, Aja, AladdinSane, ANightAtTheOpera, AtFolsomPrison, AtSanQuentin, Autobahn, AWizardATrueStar, BadReputation, Boston, CityOfNewOrleans, Columbia, CountdownToEcstasy, DamnTheTorpedoes, Desire, Desperado, DontLookBack, DreamboatAnnie, EndlessSummer, EverybodyKnowsThisIsNowhere, Fragile, GotToBeThere, GreatestHits, Hemispheres, InTheCourtOfTheCrimsonKing, LAWoman, LedZeppelinIV, LifeAndTimes, Messiah, MovingPictures, MrSpocksMusicFromOuterSpace, NoDiggity, Pearl, PermanentWaves, PicturesAtAnExhibition, PiecesOfEight, Siren, SmashHits, SomethingAnything, SpaceOddity, SpeakAndSpell, SurrealisticPillow, SymphonyNo5, Tarkus, TheBestOfMuddyWaters, TheCreation, TheDarkSideOfTheMoon, TheEmpireStrikesBackSoundtrack, TheGrandIllusion, TheNumberOfTheBeast, ThePlanets, TheRevolutionWillNotBeTelevised, TheRiseAndFallOfZiggyStardustAndTheSpidersFromMars, TheWall, TheYardbirdsGreatestHits, TheYesAlbum, Thriller, WhosNext, YouDontMessAroundwithJim];

function tableSetter()/*fork on the left; knife on the right!*/{
	var numberOfColumns = 5; /*Controls number of images per row. Manually set for consistency. (screen.width/librarian.width) can be used for a variable-width tables*/
	var currentAlbumCounter = 0;
    var albumCarpenter = document.createElement("table");
	albumCarpenter.id=("albumTable");
	document.getElementById("highFidelity").appendChild(albumCarpenter);
	
	/*Auto-populates new rows if more elements are added*/
	for (j=0;  j<lpArray.length/numberOfColumns; j++) {
	var albumCrew = document.createElement("tr");
	albumCrew.id=("Row"+j);
	document.getElementById("albumTable").appendChild(albumCrew);
	
		/*Creates cells and fills them with a div that keeps everything organized*/
		for (i=0; i<numberOfColumns; i++) {
			var albumWarden = document.createElement ("td");
			var albumLibrarian = document.createElement ("div");
			albumLibrarian.id=("lib" + currentAlbumCounter);
			albumLibrarian.className="librarian";
		
			/*Reads an album title from the array, and assigns the proper picture to it*/
			var albumNode = document.createElement ("img");
			albumNode.id=("pic" + currentAlbumCounter);
			albumNode.setAttribute("src", "Albums/"+lpArray[currentAlbumCounter].albumSrc);
			albumNode.className="album";
			albumLibrarian.appendChild(albumNode);
	
			/*Attaches album info to appropriate container, and moves on to the next album*/
			var albumFacts=document.createElement("p");
			albumFacts.id=currentAlbumCounter;
			albumFacts.className="waxFacts";
			albumFacts.innerHTML=(lpArray[currentAlbumCounter].title+" <br> "+lpArray[currentAlbumCounter].artist+" || "+lpArray[currentAlbumCounter].releaseDate);
			albumLibrarian.appendChild(albumFacts);
			albumWarden.appendChild(albumLibrarian);
			document.getElementById("Row"+j).appendChild(albumWarden);
			currentAlbumCounter++;
		}
	}
}

</script>


