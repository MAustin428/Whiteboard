var SomethingAnything = new lp("Something/ Anything?", "Todd Rundgren", 1972, "BigSomethingAnything.jpg"); 
var SpaceOddity = new lp("Space Oddity", "David Bowie", 1972, "BigSpaceOddity.jpg"); 
var SpeakAndSpell = new lp("Speak & Spell", "Depeche Mode", 1981, "BigSpeakAndSpell.jpg"); 
var SurrealisticPillow = new lp("Surrealistic Pillow", "Jefferson Aeroplane", 1967, "BigSurrealisticPillow.jpg"); 
